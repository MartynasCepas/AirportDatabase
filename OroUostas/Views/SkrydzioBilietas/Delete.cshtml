@model OroUostas.ViewModels.SkrydzioBilietasEditViewModel

@{
    ViewBag.Title = "Skrydzio Bilieto Salinimas";
}

    <h2>Skrydzio Bilieto Salinimas</h2>

    <h3>Ar tikrai norite pašalinti Bilieta?</h3>
<div>
    <h4>SkrydzioBilietasEditViewModel</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.fk_bilietas)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fk_bilietas)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.fk_skrydis)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fk_skrydis)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            @if (ViewBag.naudojama !=null)
            {
                <h5>ViewBag.naudojama</h5>
            }
            else
            {
                <input type="submit" value="Šalinti" class="btn btn-danger" />
            }
            @Html.ActionLink("Atgal į sąrašą", "Index", "SkrydzioBilietas", new { @class="btn btn-default"})
        </div>
    }
</div>
