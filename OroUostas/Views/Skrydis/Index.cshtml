@model IEnumerable<OroUostas.ViewModels.SkrydisViewModel>

@{
    ViewBag.Title = "Skrydis";
}

<h2>Skrydziai</h2>

<p>
    @Html.ActionLink("Naujas Skrydis", "Create", "Bilietas", new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lektuvas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kryptis)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.data)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lektuvas)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.kryptis)
    </td>
    <td>
        @Html.ActionLink("Redaguoti", "Edit", new { id = item.id }, new { @class= "btn btn-primary action" }) 
        @Html.ActionLink("Šalinti", "Delete", new { id = item.id }, new { @class= "btn btn-danger action" })
    </td>
</tr>
}

</table>
